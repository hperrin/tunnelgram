{
  "name": "tunnelgram",
  "version": "1.0.0",
  "description": "An easy, secure, end to end encrypted messenger.",
  "author": "Hunter Perrin <hperrin@gmail.com>",
  "license": "Apache-2.0",
  "scripts": {
    "run-(not ready)": "WEBPACK_HOT=true webpack-dev-server",
    "analyze": "WEBPACK_ANALYZE=true NODE_ENV=production webpack",
    "build": "webpack",
    "build-prod": "NODE_ENV=production webpack",
    "watch": "webpack --watch",
    "lint": "npm run lint:js && npm run lint:php",
    "lint:js": "prettier --check \"./{*.{html,config.js},src/**/*.{html,svelte,js,css,scss}}\"",
    "lint:php": "phpcs -vs --extensions=php --ignore=vendor/*,node_modules/* --runtime-set ignore_warnings_on_exit 1 .",
    "format": "npm run format:js && npm run format:php",
    "format:js": "prettier --write \"./{*.{html,config.js},src/**/*.{html,svelte,js,css,scss}}\"",
    "format:php": "phpcbf -vs --extensions=php --ignore=vendor/*,node_modules/* --runtime-set ignore_warnings_on_exit 1 .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hperrin/tunnelgram.git"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-regular": "^5.0.13",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "ffmpeg.js-hperrin": "^3.1.9001",
    "github-markdown-css": "^3.0.1",
    "nymph-client": "^5.1.0",
    "pform": "^3.3.0",
    "photoswipe": "^4.1.3",
    "pnotify": "^4.0.0",
    "tilmeld-client": "^2.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-classes": "^7.5.5",
    "@babel/preset-env": "^7.6.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "base64-js": "^1.3.1",
    "bootstrap": "^4.3.1",
    "bootstrap.native": "^2.0.27",
    "bootswatch": "^4.3.1",
    "css-loader": "^3.2.0",
    "html-loader": "^0.5.5",
    "jsencrypt": "^3.0.0-rc.1",
    "localforage": "^1.7.3",
    "mini-css-extract-plugin": "^0.8.0",
    "navigo": "^7.1.2",
    "node-emoji": "^1.10.0",
    "node-sass": "^4.13.1",
    "prettier": "^1.18.2",
    "prettier-plugin-svelte": "^0.7.0",
    "qrcode": "^1.4.2",
    "sass-loader": "^8.0.0",
    "showdown": "^1.9.1",
    "showdown-xss-filter": "^0.2.0",
    "style-loader": "^1.0.0",
    "svelte": "^3.12.1",
    "svelte-loader": "^2.13.6",
    "terser-webpack-plugin": "^2.2.1",
    "tilmeld-components": "^2.0.0",
    "timeago.js": "^4.0.1",
    "tinygesture": "^1.1.4",
    "ua-parser-js": "^0.7.20",
    "val-loader": "^1.1.1",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  }
}
